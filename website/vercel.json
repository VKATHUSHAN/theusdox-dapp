{
  "version": 2,
  "name": "theusdox-website",
  "build": {
    "env": {
      "VITE_ENVIRONMENT": "production",
      "VITE_DEBUG": "false",
      "VITE_NETWORK": "bsc-mainnet"
    }
  },
  "env": {
    "VITE_BSC_RPC": "@vite_bsc_rpc",
    "VITE_BSC_TESTNET_RPC": "@vite_bsc_testnet_rpc",
    "VITE_ETHEREUM_RPC": "@vite_ethereum_rpc",
    "VITE_USDOX_CONTRACT": "@vite_usdox_contract",
    "VITE_VAULT_CONTRACT": "@vite_vault_contract",
    "VITE_ORACLE_CONTRACT": "@vite_oracle_contract",
    "VITE_ALCHEMY_KEY": "@vite_alchemy_key",
    "VITE_INFURA_API_KEY": "@vite_infura_api_key",
    "VITE_WALLETCONNECT_PROJECT_ID": "@vite_walletconnect_project_id",
    "VITE_0X_API_KEY": "@vite_0x_api_key",
    "VITE_APP_NAME": "TheUSDOX",
    "VITE_APP_VERSION": "1.0.0",
    "VITE_APP_DESCRIPTION": "The On-Chain Dollar Revolution",
    "VITE_DOMAIN": "theusdox.com",
    "VITE_DEVELOPER_EMAIL": "dkathushan@gmail.com",
    "VITE_TWITTER_URL": "https://twitter.com/theusdox",
    "VITE_DISCORD_URL": "https://discord.gg/usdox",
    "VITE_GITHUB_URL": "https://github.com/VKATHUSHAN/theusdox-dapp",
    "VITE_DOCS_URL": "https://docs.theusdox.com"
  },
  "functions": {
    "src/pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "redirects": [
    {
      "source": "/home",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}